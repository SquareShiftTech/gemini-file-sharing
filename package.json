{
  "name": "gcs-deployer",
  "version": "0.1.0",
  "description": "Example MCP Server for Gemini CLI Extension",
  "type": "module",
  "bin": {
    "gcs-deployer": "./dist/gcs-deploy-bundled.cjs"
  },
  "files": [
    "dist"
  ],
  "main": "dist/gcs-deploy-bundled.cjs",
  "scripts": {
    "build": "tsc",
    "bundle": "esbuild src/gcs-deploy.ts --bundle --platform=node --target=node18 --outfile=dist/gcs-deploy-bundled.cjs --format=cjs --external:mock-aws-s3 --external:nock --external:aws-sdk",
    "package": "node scripts/package.js"
  },
  "devDependencies": {
    "@types/node": "^20.11.25",
    "esbuild": "^0.27.2",
    "typescript": "~5.4.5"
  },
  "dependencies": {
    "@google-cloud/storage": "^7.18.0",
    "@modelcontextprotocol/sdk": "^1.23.0",
    "@types/mime-types": "^3.0.1",
    "mime-types": "^3.0.2",
    "zod": "^3.22.4"
  }
}